{
  "configType": "navigation_configuration",
  "version": "3.1.0",
  "lastUpdated": "2025-01-27",
  "relatedWorkflow": "12_Frontend_Development.md",
  
  "navigationStructure": {
    "type": "hierarchical",
    "maxDepth": 3,
    "strategy": "progressive disclosure",
    "persistence": "localStorage",
    "responsive": true
  },

  "primaryNavigation": {
    "type": "horizontal",
    "position": "header",
    "sticky": true,
    "items": [
      {
        "id": "dashboard",
        "label": "Dashboard",
        "path": "/dashboard",
        "icon": "dashboard",
        "roles": ["user", "admin"],
        "exact": true
      },
      {
        "id": "projects",
        "label": "Projects",
        "path": "/projects",
        "icon": "folder",
        "roles": ["user", "admin"],
        "children": [
          {
            "id": "active-projects",
            "label": "Active Projects",
            "path": "/projects/active",
            "icon": "play"
          },
          {
            "id": "archived-projects",
            "label": "Archived",
            "path": "/projects/archived",
            "icon": "archive"
          },
          {
            "id": "create-project",
            "label": "Create New",
            "path": "/projects/create",
            "icon": "plus"
          }
        ]
      },
      {
        "id": "analytics",
        "label": "Analytics",
        "path": "/analytics",
        "icon": "chart",
        "roles": ["admin"],
        "children": [
          {
            "id": "overview",
            "label": "Overview",
            "path": "/analytics/overview",
            "icon": "overview"
          },
          {
            "id": "reports",
            "label": "Reports",
            "path": "/analytics/reports",
            "icon": "report"
          },
          {
            "id": "insights",
            "label": "Insights",
            "path": "/analytics/insights",
            "icon": "lightbulb"
          }
        ]
      },
      {
        "id": "settings",
        "label": "Settings",
        "path": "/settings",
        "icon": "settings",
        "roles": ["user", "admin"],
        "children": [
          {
            "id": "profile",
            "label": "Profile",
            "path": "/settings/profile",
            "icon": "user"
          },
          {
            "id": "preferences",
            "label": "Preferences",
            "path": "/settings/preferences",
            "icon": "preferences"
          },
          {
            "id": "security",
            "label": "Security",
            "path": "/settings/security",
            "icon": "shield"
          }
        ]
      }
    ]
  },

  "secondaryNavigation": {
    "type": "sidebar",
    "position": "left",
    "collapsible": true,
    "defaultCollapsed": false,
    "width": {
      "expanded": "256px",
      "collapsed": "64px"
    },
    "overlay": {
      "mobile": true,
      "tablet": false,
      "desktop": false
    },
    "items": [
      {
        "id": "quick-actions",
        "label": "Quick Actions",
        "type": "section",
        "children": [
          {
            "id": "new-project",
            "label": "New Project",
            "path": "/projects/create",
            "icon": "plus",
            "shortcut": "Ctrl+N"
          },
          {
            "id": "search",
            "label": "Search",
            "path": "/search",
            "icon": "search",
            "shortcut": "Ctrl+K"
          }
        ]
      },
      {
        "id": "recent",
        "label": "Recent",
        "type": "dynamic",
        "maxItems": 5,
        "source": "recentProjects"
      },
      {
        "id": "favorites",
        "label": "Favorites",
        "type": "dynamic",
        "maxItems": 10,
        "source": "favoriteProjects"
      }
    ]
  },

  "breadcrumbNavigation": {
    "enabled": true,
    "separator": "/",
    "maxItems": 5,
    "showHome": true,
    "homeLabel": "Home",
    "homePath": "/dashboard",
    "truncation": "middle",
    "responsive": {
      "mobile": "last 2 items",
      "tablet": "last 3 items",
      "desktop": "all items"
    }
  },

  "tabNavigation": {
    "type": "horizontal",
    "variant": "underline",
    "scrollable": true,
    "centered": false,
    "fullWidth": false,
    "lazy": true,
    "persistence": "sessionStorage"
  },

  "contextualNavigation": {
    "enabled": true,
    "position": "right-sidebar",
    "width": "200px",
    "items": [
      {
        "type": "table-of-contents",
        "autoGenerate": true,
        "maxDepth": 3,
        "smooth": true
      },
      {
        "type": "related-links",
        "source": "dynamic",
        "maxItems": 5
      },
      {
        "type": "actions",
        "items": [
          {
            "id": "share",
            "label": "Share",
            "icon": "share"
          },
          {
            "id": "export",
            "label": "Export",
            "icon": "download"
          }
        ]
      }
    ]
  },

  "mobileNavigation": {
    "type": "drawer",
    "trigger": "hamburger",
    "position": "left",
    "overlay": true,
    "swipeToOpen": true,
    "swipeToClose": true,
    "backdrop": true,
    "animation": "slide",
    "items": "inherit from primaryNavigation"
  },

  "navigationBehavior": {
    "activeState": {
      "strategy": "path-based",
      "exact": false,
      "highlight": "background-color",
      "indicator": "left-border"
    },
    "hover": {
      "enabled": true,
      "delay": 200,
      "animation": "fade"
    },
    "focus": {
      "visible": true,
      "style": "outline",
      "color": "#2563eb",
      "width": "2px"
    },
    "loading": {
      "indicator": "spinner",
      "position": "inline",
      "timeout": 5000
    }
  },

  "keyboardNavigation": {
    "enabled": true,
    "shortcuts": {
      "openMenu": "Alt+M",
      "closeMenu": "Escape",
      "nextItem": "ArrowDown",
      "previousItem": "ArrowUp",
      "expandItem": "ArrowRight",
      "collapseItem": "ArrowLeft",
      "selectItem": "Enter",
      "search": "Ctrl+K"
    },
    "skipLinks": [
      {
        "label": "Skip to main content",
        "target": "#main-content"
      },
      {
        "label": "Skip to navigation",
        "target": "#navigation"
      }
    ]
  },

  "searchNavigation": {
    "enabled": true,
    "global": true,
    "placeholder": "Search...",
    "shortcut": "Ctrl+K",
    "categories": [
      "pages",
      "projects",
      "users",
      "settings"
    ],
    "recentSearches": {
      "enabled": true,
      "maxItems": 5
    },
    "suggestions": {
      "enabled": true,
      "maxItems": 8
    }
  },

  "routingConfiguration": {
    "library": "react-router-dom",
    "version": "6.8.0",
    "mode": "browser",
    "basename": "/",
    "caseSensitive": false,
    "lazyLoading": true,
    "preloading": "hover",
    "errorBoundary": true,
    "notFound": "/404",
    "unauthorized": "/401"
  },

  "routeGuards": {
    "authentication": {
      "enabled": true,
      "redirectTo": "/login",
      "publicRoutes": ["/", "/login", "/register", "/forgot-password"]
    },
    "authorization": {
      "enabled": true,
      "strategy": "role-based",
      "redirectTo": "/unauthorized",
      "fallback": "/dashboard"
    },
    "maintenance": {
      "enabled": false,
      "redirectTo": "/maintenance",
      "allowedRoles": ["admin"]
    }
  },

  "linkBehavior": {
    "external": {
      "target": "_blank",
      "rel": "noopener noreferrer",
      "icon": true,
      "warning": false
    },
    "internal": {
      "prefetch": "hover",
      "preload": "visible",
      "scroll": "top"
    },
    "download": {
      "icon": true,
      "tracking": true
    }
  },

  "navigationAnalytics": {
    "enabled": true,
    "events": [
      "navigation_click",
      "menu_open",
      "menu_close",
      "search_query",
      "breadcrumb_click"
    ],
    "heatmap": {
      "enabled": true,
      "service": "hotjar"
    },
    "userFlow": {
      "enabled": true,
      "tracking": "google-analytics"
    }
  },

  "accessibility": {
    "ariaLabels": {
      "mainNavigation": "Main navigation",
      "secondaryNavigation": "Secondary navigation",
      "breadcrumb": "Breadcrumb navigation",
      "pagination": "Pagination navigation",
      "skipLinks": "Skip navigation links"
    },
    "landmarks": {
      "navigation": "navigation",
      "search": "search",
      "banner": "banner"
    },
    "announcements": {
      "routeChange": true,
      "menuState": true,
      "searchResults": true
    },
    "focusManagement": {
      "trapFocus": "modals and drawers",
      "restoreFocus": true,
      "skipToContent": true
    }
  },

  "performance": {
    "lazyLoading": {
      "enabled": true,
      "threshold": "50px",
      "placeholder": "skeleton"
    },
    "caching": {
      "menuData": "localStorage",
      "userPreferences": "localStorage",
      "recentItems": "sessionStorage"
    },
    "bundleSplitting": {
      "routeLevel": true,
      "componentLevel": false
    },
    "preloading": {
      "strategy": "hover",
      "delay": 200,
      "priority": "high"
    }
  },

  "customization": {
    "themes": {
      "light": {
        "background": "#ffffff",
        "text": "#1f2937",
        "accent": "#2563eb",
        "hover": "#f3f4f6"
      },
      "dark": {
        "background": "#1f2937",
        "text": "#f9fafb",
        "accent": "#3b82f6",
        "hover": "#374151"
      }
    },
    "userPreferences": {
      "collapsedSidebar": "localStorage",
      "preferredTheme": "localStorage",
      "recentItems": "sessionStorage"
    },
    "branding": {
      "logo": {
        "light": "/assets/logo-light.svg",
        "dark": "/assets/logo-dark.svg",
        "alt": "DafnckMachine Logo"
      },
      "colors": {
        "primary": "#2563eb",
        "secondary": "#64748b"
      }
    }
  },

  "testing": {
    "e2e": {
      "framework": "Cypress",
      "scenarios": [
        "navigation flow",
        "mobile menu",
        "keyboard navigation",
        "search functionality",
        "route guards"
      ]
    },
    "unit": {
      "framework": "Jest + React Testing Library",
      "coverage": [
        "navigation components",
        "routing logic",
        "guard functions"
      ]
    },
    "accessibility": {
      "automated": "jest-axe",
      "manual": "screen reader testing"
    }
  },

  "documentation": {
    "storybook": {
      "navigationStories": true,
      "interactiveExamples": true,
      "accessibilityDocs": true
    },
    "routeMap": {
      "generated": true,
      "format": "sitemap.xml",
      "includePrivate": false
    },
    "userGuide": {
      "keyboardShortcuts": true,
      "navigationTips": true,
      "troubleshooting": true
    }
  }
} 