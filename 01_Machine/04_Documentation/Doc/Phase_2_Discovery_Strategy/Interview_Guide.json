  "participant_segments": {
    "individual_developers": {
      "segment_description": "Solo developers, freelancers, and individual contributors",
      "specific_focus_areas": [
        "Personal productivity and efficiency",
        "Tool adoption decision-making",
        "Learning curve tolerance",
        "Budget constraints and value perception"
      ],
      "customized_questions": [
        "How do you typically evaluate and adopt new development tools?",
        "What's your biggest challenge when working on projects alone?",
        "How much time do you spend on non-coding activities during development?"
      ]
    },
