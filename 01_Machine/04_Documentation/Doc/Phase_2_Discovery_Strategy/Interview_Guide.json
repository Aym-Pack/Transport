    "interview_techniques": {
      "active_listening": [
        "Give full attention to participant responses",
        "Use verbal and non-verbal encouragement",
        "Avoid interrupting or rushing responses",
        "Reflect back what you hear to confirm understanding"
      ],
      "probing_strategies": [
        "Use open-ended follow-up questions",
        "Ask for specific examples and stories",
        "Explore the 'why' behind responses",
        "Dig deeper into unexpected or interesting responses"
      ],
      "neutrality_maintenance": [
        "Avoid leading questions or biased language",
        "Don't express personal opinions or judgments",
        "Remain curious and non-judgmental",
        "Validate all responses as valuable insights"
      ],
      "time_management": [
        "Keep track of time throughout interview",
        "Prioritize most important questions if running short",
        "Allow natural conversation flow while staying on track",
        "Signal time transitions to participant"
